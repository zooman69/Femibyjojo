# Twilio SMS Configuration
# Sign up at https://www.twilio.com/ to get these credentials

# Your Twilio Account SID (found in Twilio Console)
TWILIO_ACCOUNT_SID=your_account_sid_here

# Your Twilio Auth Token (found in Twilio Console)
TWILIO_AUTH_TOKEN=your_auth_token_here

# Your Twilio Phone Number (must be in E.164 format, e.g., +15551234567)
TWILIO_PHONE_NUMBER=+15551234567

# Phone number to receive login notifications (E.164 format, e.g., +15559876543)
NOTIFICATION_PHONE_NUMBER=+15559876543
